<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.liangdeqi.sensormanager_v1">

    <uses-permission android:name="android.permission.SENSOR_INFO" />
    <uses-permission android:name="android.permission.SENSOR_ENABLE" />

    <application
        android:name="org.litepal.LitePalApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AccelerometerActivity" />
        <activity android:name=".BaseActivity" />
        <activity android:name=".OrientationActivity" />
        <activity android:name=".GyroscopeActivity" />
        <activity android:name=".GravityActivity" />
        <activity android:name=".MagneticFieldActivity" />
        <activity android:name=".LinearAccelerometerActivity" />
        <activity android:name=".AtLPActivity"></activity>
    </application>

</manifest>